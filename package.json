{
  "name": "diffex",
  "description": "Diffex, ERC20 token options trading platform",
  "version": "0.0.2",
  "homepage": "https://www.diffex.io",
  "private": true,
  "keywords": [
    "html5",
    "css3",
    "es6",
    "frontend",
    "sass",
    "less",
    "static",
    "boilerplate",
    "webpack"
  ],
  "scripts": {
    "prestart": "npm run lint:fix",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js",
    "start:dist": "cross-env NODE_ENV=production webpack-dev-server --config config/webpack.config.js && http-server ./dist -o",
    "setup": "node config/site.setup.js",
    "lint:fix": "npm run lint:js:fix && npm run lint:styles:fix",
    "lint:js": "./node_modules/.bin/eslint \"src/javascripts/*.js\"",
    "lint:js:fix": "./node_modules/.bin/eslint --fix \"src/javascripts/*.js\"",
    "lint:styles": "stylelint \"src/stylesheets/*.scss\"",
    "lint:styles:fix": "stylelint --fix \"src/stylesheets/*.{css,scss,sass}\"",
    "prebuild:dist": "npm install",
    "build:dist": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "deploy": "node config/site.deploy.js"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "clear": "^0.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "enquirer": "^2.3.6",
    "favicons-webpack-plugin": "^4.2.0",
    "figlet": "^1.2.3",
    "file-loader": "^4.2.0",
    "ftp-deploy": "^2.4.1",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "http-server": "^0.12.3",
    "image-webpack-loader": "^5.0.0",
    "jquery": "^3.6.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-sass": "^0.4.1",
    "reset-css": "^5.0.1",
    "robotstxt-webpack-plugin": "^6.0.0",
    "sanitize.css": "^12.0.0",
    "sass-loader": "^7.2.0",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^1.0.0",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.2.2"
  }
}
